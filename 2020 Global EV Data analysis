data=read.csv(file.choose())
#Select the csv file using the pop-up window
View(data)
attach(data)
head(data)
names
#Taking only the Numerical variables.
data1=subset(data,select=-c(Brand,Model,FastCharge_KmH,Segment,RapidCharge,PowerTrain,BodyStyle,PriceEuro))
View(data1)
summary(data)
attach(data1)
mod1<-lm(PriceEuro~Range_Km,data=data1)
summary(mod1)
mod2<-lm(PriceEuro~AccelSec,data=data1)
summary(mod2)
mod3<-lm(PriceEuro~TopSpeed_KmH,data=data1)
summary(mod3)
mod4<-lm(PriceEuro~Efficiency_WhKm,data=data1)
summary(mod4)
plot1=ggplot(data,aes(Efficiency_WhKm))+geom_density(fill="blue", alpha=0.3)+xlim(100,300)+labs(x="Efficiency",y="Distribution of data")+ggtitle("Distribution of Efficiency")+geom_vline(xintercept = median(Efficiency_WhKm))
plot2=ggplot(data,aes(Range_Km))+geom_density(fill="blue", alpha=0.3)+xlim(-100,1100)+labs(x="Range",y="Distribution of data")+ggtitle("Distribution of Range")+geom_vline(xintercept = median(Range_Km))
plot3=ggplot(data,aes(TopSpeed_KmH))+geom_density(fill="blue", alpha=0.3)+xlim(0,500)+labs(x="TopSpeed",y="Distribution of data")+ggtitle("Distribution of TopSpeed")+geom_vline(xintercept = median(TopSpeed_KmH))
plot4=ggplot(data,aes(PriceEuro))+geom_density(fill="blue", alpha=0.3)+labs(x="Price",y="Distribution of data")+ggtitle("Distribution of Price")+geom_vline(xintercept = median(PriceEuro))+xlim(0,150000)
library(ggcorrplot)
cor=cor(data1)
View(cor)
library(wesanderson)
ggcorrplot(corr,colors = wes_palette(name="Darjeeling2",5),lab = T,lab_col="white",lab_size=3,ggtheme = ggplot2::theme_light(),hc.order = T)
mod<-lm(log(PriceEuro)~Range_Km+AccelSec+TopSpeed_KmH+Efficiency_WhKm,data=data1)
summary(mod)
# Diagnostic plots
plot(mod)
library(car)
#Variance Inflation factor
vif(mod)
lmtest::bptest(mod)
car::durbinwatsontest(mod)
